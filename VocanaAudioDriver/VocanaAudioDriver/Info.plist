<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>com.vocana.audio.driver</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>$(PRODUCT_NAME)</string>
	<key>CFBundlePackageType</key>
	<string>$(PRODUCT_BUNDLE_PACKAGE_TYPE)</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0</string>
	<key>CFBundleVersion</key>
	<string>1</string>
	<key>OSBundleUsageDescription</key>
	<string>Vocana Audio Driver provides virtual audio devices for noise cancellation</string>

	<!-- Driver Extension Configuration -->
	<key>NSExtension</key>
	<dict>
		<key>NSExtensionPointIdentifier</key>
		<string>com.apple.driverkit</string>
		<key>NSExtensionAttributes</key>
		<dict>
			<key>IOUserClass</key>
			<string>VocanaAudioDriver</string>
		</dict>
	</dict>

	<!-- DriverKit Configuration -->
	<key>IOKitPersonalities</key>
	<dict>
		<key>VocanaAudioDriver</key>
		<dict>
			<key>CFBundleIdentifier</key>
			<string>com.vocana.audio.driver</string>
			<key>CFBundleIdentifierKernel</key>
			<string>com.vocana.audio.driver</string>
			<key>IOClass</key>
			<string>IOUserService</string>
			<key>IOProviderClass</key>
			<string>IOUserResources</string>
			<key>IOUserClass</key>
			<string>VocanaAudioDriver</string>
			<key>IOUserServerName</key>
			<string>com.vocana.audio.driver</string>
			<key>UserClientProperties</key>
			<dict>
				<key>IOClass</key>
				<string>IOUserClient</string>
				<key>IOUserClass</key>
				<string>VocanaAudioUserClient</string>
			</dict>
		</dict>
	</dict>

	<!-- Audio Driver Configuration -->
	<key>AudioDriverProperties</key>
	<dict>
		<key>VirtualDevices</key>
		<array>
			<dict>
				<key>Name</key>
				<string>Vocana Microphone</string>
				<key>UID</key>
				<string>com.vocana.audio.input</string>
				<key>DeviceType</key>
				<string>Input</string>
				<key>SampleRate</key>
				<integer>48000</integer>
				<key>Channels</key>
				<integer>2</integer>
				<key>BitsPerChannel</key>
				<integer>32</integer>
				<key>FormatFlags</key>
				<integer>12</integer>
			</dict>
			<dict>
				<key>Name</key>
				<string>Vocana Speaker</string>
				<key>UID</key>
				<string>com.vocana.audio.output</string>
				<key>DeviceType</key>
				<string>Output</string>
				<key>SampleRate</key>
				<integer>48000</integer>
				<key>Channels</key>
				<integer>2</integer>
				<key>BitsPerChannel</key>
				<integer>32</integer>
				<key>FormatFlags</key>
				<integer>12</integer>
			</dict>
		</array>
	</dict>
</dict>
</plist>