<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>IOKitPersonalities</key>
	<dict>
		<key>VocanaAudioDriver</key>
		<dict>
			<key>CFBundleIdentifierKernel</key>
			<string>com.apple.kpi.iokit</string>
			<key>IOClass</key>
			<string>IOUserService</string>
			<key>IOMatchCategory</key>
			<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
			<key>IOProviderClass</key>
			<string>IOUserResources</string>
			<key>IOResourceMatch</key>
			<string>IOKit</string>
			<key>IOUserClass</key>
			<string>VocanaAudioDriver</string>
			<key>IOUserServerName</key>
			<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
		</dict>
	</dict>

	<!-- Audio Driver Configuration -->
	<key>AudioDriverProperties</key>
	<dict>
		<key>VirtualDevices</key>
		<array>
			<dict>
				<key>Name</key>
				<string>Vocana Microphone</string>
				<key>UID</key>
				<string>com.vocana.audio.input</string>
				<key>DeviceType</key>
				<string>Input</string>
				<key>SampleRate</key>
				<integer>48000</integer>
				<key>Channels</key>
				<integer>2</integer>
				<key>BitsPerChannel</key>
				<integer>32</integer>
				<key>FormatFlags</key>
				<integer>12</integer>
			</dict>
			<dict>
				<key>Name</key>
				<string>Vocana Speaker</string>
				<key>UID</key>
				<string>com.vocana.audio.output</string>
				<key>DeviceType</key>
				<string>Output</string>
				<key>SampleRate</key>
				<integer>48000</integer>
				<key>Channels</key>
				<integer>2</integer>
				<key>BitsPerChannel</key>
				<integer>32</integer>
				<key>FormatFlags</key>
				<integer>12</integer>
			</dict>
		</array>
	</dict>

	<key>OSBundleUsageDescription</key>
	<string>Vocana Audio Driver provides virtual audio devices for real-time noise cancellation.</string>
</dict>
</plist>
